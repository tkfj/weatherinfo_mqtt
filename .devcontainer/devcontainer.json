{
	"name": "weatherinfo_mqtt_dev",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/usr/src/app",
	"remoteUser":"appuser",
	"updateRemoteUserUID":true,
	"overrideCommand": true,

	"customizations": {
		"vscode": {
			"settings": {
				// 好みのシェルを設定
				"terminal.integrated.shell.linux": "/bin/bash"
			},
		
			"extensions": [
				"ms-python.python"
			]
		}
	}
}
